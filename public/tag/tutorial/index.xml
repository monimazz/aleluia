  mutate(peso = as.numeric(subject)) %&amp;gt;%
  ggplot(aes(x=value, y=peso)) +
  geom_segment( aes(xend=value, yend=0)) +
  geom_point( size=4, color=&amp;quot;orange&amp;quot;) +
